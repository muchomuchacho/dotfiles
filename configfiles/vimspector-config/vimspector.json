{
  "configurations": {
    "<name>: Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Python: Current File",
        "type": "python",
        "stopOnEntry": true,
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "python": "$HOME/.virtualenvs/sarsa/bin/python",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
